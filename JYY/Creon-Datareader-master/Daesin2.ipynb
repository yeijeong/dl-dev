    "import pandas as pd\n",
    "\n",
    "#데이터 프레임 만들기\n",
    "stock_df = pd.DataFrame({'date':df['date'],'time':df['time'],'open':df['open'],'high':df['high'], 'low':df['low'],'close':df['close'],'value':df['value']})\n",
    "stock_df\n",
    "\n",
    "stock_df.to_csv('2years_ago_past_data.csv', index=False)"
